{
	"name": "oliverthiele/ot-sitekitcecard",
	"type": "typo3-cms-extension",
	"description": "TYPO3 content element that displays a card element. Integrates with the Sitekit and ot-irrebuttons extensions.",
	"homepage": "https://www.oliver-thiele.de/",
	"license": [
		"GPL-2.0-or-later"
	],
	"authors": [
		{
			"name": "Oliver Thiele",
			"email": "mail@oliver-thiele.de",
			"homepage": "https://www.oliver-thiele.de",
			"role": "Developer"
		}
	],
	"keywords": [
		"TYPO3 CMS",
		"content element",
		"card",
		"grid",
		"sitekit",
		"frontend"
	],
	"require": {
		"typo3/cms-core": "^13.4",
		"oliverthiele/ot-irrebuttons": "^3.2 || dev-main",
		"oliverthiele/ot-sitekit-base": "*"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "ot_sitekitcecard"
		}
	}
}
